<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dataProcessor" class="processor.restriction.upload.RestrictionUploadDataProcessor">
        <property name="processingEngineChain" ref="processingEngineChain"></property>
    </bean>

    <bean id="processingEngineChain" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="engine.ProcessingEngine"/>
    </bean>

</beans>